<% if logged_in? %>
  <div class="row">
        <div class="col-md-12">
      <h3>Daily Logs</h3>
      <%= render 'shared/feed' %>
    </div>
  </div>
  
  
<% else %>
<div class="center jumbotron">
  <h1>Welcome to the Online Journal</h1>

  <h2>
    This is the home page for the
    Online Journal
  </h2>

  <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-primary" %>

  <h2>Already signed up?</h2>

  <%= link_to "Log in", login_path, class: "btn btn-md btn-primary" %>
</div>

<% end %>